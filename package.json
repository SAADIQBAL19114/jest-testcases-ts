{
  "name": "sequelize-orm",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run db:migrate && npx tsc && node ./dist/index.js",
    "test": "cross-env NODE_ENV=test jest --no-cache --config ./jest.config.js --detectOpenHandles",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:create": "cross-env NODE_ENV=development npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "mysql2": "^3.6.5",
    "node": "^21.2.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.1",
    "ts-jest": "^29.1.5",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  }
}
